page{background:#f5f5f5}
.search-box{margin:24rpx}
.search-box input{background:#fff;padding:16rpx;border-radius:12rpx;width:100%}
.result-list{max-height:300rpx;margin-top:16rpx}
.club-card{display:flex;justify-content:space-between;align-items:center;background:#fff;border-radius:12rpx;padding:24rpx;margin-bottom:16rpx;box-shadow:0 6rpx 12rpx rgba(0,0,0,.04)}
.apply-btn{background:#07C160;color:#fff;border-radius:8rpx;padding:12rpx 24rpx;font-size:26rpx}
.section-title{margin:24rpx;font-size:30rpx;color:#333}
.my-clubs{margin:0 24rpx 24rpx 24rpx}
.my-card{background:#fff;border-radius:12rpx;padding:24rpx;box-shadow:0 6rpx 12rpx rgba(0,0,0,.04);margin-bottom:24rpx}
.my-card .main{display:flex;justify-content:space-between;align-items:flex-start}
.my-card .info{flex:1}
.my-card .name{font-size:32rpx;font-weight:600}
.my-card .slogan{font-size:26rpx;color:#666;margin-top:6rpx}
.my-card .stats{font-size:24rpx;color:#888;margin-top:6rpx;line-height:32rpx}
.my-card .role{font-size:26rpx;color:#888;margin-left:20rpx;white-space:nowrap}
